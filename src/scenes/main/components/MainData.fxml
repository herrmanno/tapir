<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.* ?>
<?import javafx.scene.control.* ?>
<?import javafx.geometry.Insets ?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" type="HBox" fillHeight="false" prefWidth="798.0" spacing="15.0">
    <children>
       <ComboBox fx:id="combo_method" prefWidth="100.0" promptText="Method" />
       <TextField id="text_location" fx:id="text_location" promptText="location" HBox.hgrow="ALWAYS" />
       <Button id="btn_send" fx:id="btn_send" alignment="CENTER" styleClass="z-2" text="Send" />
    </children>
    <padding>
       <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
    </padding>
</fx:root>